{
  "name": "function-starter",
  "version": "1.0.0",
  "description": "Boilerplate for TS cloud functions",
  "main": "index.js",
  "author": "Preston Holmes <preston@ptone.com>",
  "license": "Apache-2.0",
  "dependencies": {
  },
  "devDependencies": {
    "gts": "^0.9.0",
    "@google-cloud/functions-framework": "^1.1.0",
    "@types/express": "^4.16.1",
    "@types/node": "^11.13.6",
    "express": "^4.16.4",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.4"
  },
  "scripts": {
    "check": "gts check",
    "clean": "gts clean",
    "compile": "yarn tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check",
    "deploy": "gcloud functions deploy --runtime nodejs10 --trigger-http --source ./build ${FUNCTION_TARGET}",
    "start": "cd build;npx @google-cloud/functions-framework;cd .."
  }
}
